package e3Clock;

public class time {
	private int hour;
	private int minute;
	time() {};
	public void SetHour(int h){
		hour = h;
		System.out.println("Hour set to:"+hour);
	}
	public void SetMinute(int m){
		minute = m;
		System.out.println("Minute set to:"+minute);
	}
	public static void main(String arg[]){
		time t = new time();
		t.hour = 3;
		t.minute = 25;
		System.out.println("The time now is "+t.hour+":"+t.minute);
	}
}
